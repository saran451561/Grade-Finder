#include<stdio.h>
int main(){
    int mark;
    printf("ENTER MARK:");
    scanf("%d",&mark);
    if(mark>=90){
        printf("A* Grade");
    }
    else if(mark>=80||mark>=75){
        printf("B* Grade");
    }
    else if(mark>=70||mark>=60){
        printf("C* Grade");
    }
    else if(mark>=35||mark>50){
        printf("Average");
    }
    else{
        printf("Fail**");
    }
}
